<div class="ui horizontal list resource-nav-menu">
  {{#unless model.isNew}}
    {{#link-to (concat 'cities.city.' modelName) model class='item'}}
      <i class='large black circular arrow link left icon' style="background-color: white;"></i>
    {{/link-to}}
  {{/unless}}
  <div class="item" onclick={{action submitRoute model}}>
    <i class='large {{if model.hasDirtyAttributes 'green' 'black'}} circular link {{if model.isSaving 'loading spinner icon' 'save icon'}}' style="background-color: white;"></i>
  </div>
</div>
<div class="ui form">
  {{yield (hash assetTypes=assetTypes investmentTypes=investmentTypes assetSubTypes=assetSubTypes investmentSources=investmentSources parcelOwnershipTypes=parcelOwnershipTypes)}}
</div>